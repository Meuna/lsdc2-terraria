{
    "name": "terraria-ecs",
    "engineType": "ecs",
    "engine": {
        "image": "meuna/lsdc2:terraria",
        "cpu": "1 vCPU",
        "memory": "2 GB"
    },
    "ingress": {
        "tcp": [7777]
    },
    "env": {
        "LSDC2_LOW_MEMORY_WARNING_MB": "512",
        "LSDC2_LOW_MEMORY_SIGNAL_MB": "256",
        "LSDC2_SCAN_STDOUT": "true",
        "LSDC2_WAKEUP_SENTINEL": "Server started"
    },
    "params": {
        "TERRARIA_VERSION": "Terraria version (a.b.c.d)",
        "SERVER_PASS": "Password",
        "WORLD_SIZE": "Size (small-[medium]-large)",
        "WORLD_DIFFICULTY": "Difficulty ([normal]-expert-master-journey)",
        "WORLD_SEED": "Seed"
    }
}